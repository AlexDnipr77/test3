{
    "version": 2,
    "env": {},
    "builds": [
        {
            "src": "ui/build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "ui/build/index.html"
        },
        {
            "src": "/another",
            "dest": "ui/build/another.html"
        },

        {
            "src": "/(.+)",
            "dest": "ui/build/$1"
        }
    ]
}